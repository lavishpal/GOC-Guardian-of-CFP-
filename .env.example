# CFP Guardian Environment Configuration
# Copy this file to .env and fill in your actual values

# Oumi API Configuration
OUMI_API_KEY=your_oumi_api_key_here
OUMI_BASE_URL=https://api.oumi.ai
OUMI_ENABLED=true
OUMI_TIMEOUT=30
OUMI_MAX_RETRIES=3

# Optional: Conference Data Sources
# Add API keys for Sched.com or Sessionize.com if you have them
# SCHED_API_KEY=your_sched_api_key
# SESSIONIZE_API_KEY=your_sessionize_api_key

# Optional: Development Settings
# DEBUG=true
# LOG_LEVEL=INFO
